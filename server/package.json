{
"name": "mern-testing-server",
"version": "1.0.0",
"main": "src/index.js",
"scripts": {
"start": "node src/index.js",
"dev": "nodemon src/index.js",
"test": "jest --runInBand",
"test:unit": "jest --config=jest.config.server.json --runTestsByPath server/tests/unit --runInBand",
"test:integration": "jest --config=jest.config.server.json --runTestsByPath server/tests/integration --runInBand",
"coverage": "jest --config=jest.config.server.json --coverage"
},
"dependencies": {
"express": "^4.18.0",
"mongoose": "^6.0.0",
"winston": "^3.8.0"
},
"devDependencies": {
"jest": "^29.0.0",
"supertest": "^6.3.0",
"mongodb-memory-server": "^8.11.0",
"nodemon": "^2.0.22"
}
}